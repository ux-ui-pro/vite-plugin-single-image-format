{
  "name": "vite-plugin-single-image-format",
  "version": "2.3.0",
  "description": "vite-plugin-single-image-format is a Vite/Rollup plugin that converts every raster asset in your build to a single output format – webp, png or avif. It can optionally re‑compress images that are already in the target format and automatically rewrites all references in HTML/CSS/JS.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/vite-plugin-single-image-format.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/vite-plugin-single-image-format/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/vite-plugin-single-image-format",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "verify": "yarn lint && yarn typecheck",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "yarn clean && yarn build && yarn verify"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@types/node": "25.0.10",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4"
  },
  "dependencies": {
    "@ampproject/remapping": "2.3.0",
    "magic-string": "0.30.21",
    "p-limit": "7.3.0"
  },
  "peerDependencies": {
    "sharp": "0.34.5"
  },
  "optionalDependencies": {
    "sharp": "0.34.5"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "rollup-plugin",
    "sharp",
    "image",
    "format",
    "conversion",
    "optimization",
    "asset",
    "compression",
    "webp",
    "png",
    "avif",
    "webp converter",
    "avif converter",
    "build optimization",
    "frontend performance"
  ]
}
